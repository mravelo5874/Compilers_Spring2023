PUSHIMM 0
LINK
JSR main
POPFBR
STOP

main:
ADDSP 3
PUSHIMM 3
STOREOFF 2 //a
PUSHIMM 4
STOREOFF 3 //b

// code for: c = addmeth(a,b);
PUSHIMM 0
PUSHOFF 2 //a
PUSHOFF 3 //b
LINK
JSR addmeth
POPFBR
ADDSP -2

// code for: return c;
PUSHOFF 4 //c
STOREOFF -1 //rv
JUMP main_END

main_END:
STOREOFF -1 //rv
ADDSP -3
JUMPIND

addmeth:
ADDSP 1
PUSHOFF -2 //x
PUSHOFF -1 //y
ADD
STOREOFF 2 //z
PUSHOFF 2 //z
STOREOFF -3 //rv
JUMP addmeth_END

addmeth_END:
STOREOFF -3 //rv
JUMPIND